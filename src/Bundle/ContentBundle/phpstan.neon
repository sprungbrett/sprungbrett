includes:
	- vendor/jangregor/phpstan-prophecy/src/extension.neon

parameters:
    excludes_analyse:
        - %currentWorkingDirectory%/vendor/*.php
    ignoreErrors:
        - '#reveal#'
        - '#shouldBeCalled#'
        - '#ObjectProphecy#'
        - '#willReturn#'
        - '#\(void\) is used#'
        - '#shouldNotBeCalled#'
